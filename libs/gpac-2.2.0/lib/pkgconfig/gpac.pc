prefix=/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${exec_prefix}/include

Name: gpac
Description: GPAC Multimedia Framework
URL: http://gpac.io
Version:2.2
Cflags: -I${prefix}/include
Libs: -L${libdir} -lgpac
Libs.private: -lgpac_static  -lm -lGL -lGLU -lX11 -lz -lz     -lpng16 -lz      -logg -lvorbis    -lpthread
